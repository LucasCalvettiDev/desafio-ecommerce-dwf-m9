{
  "version": 2,
  "routes": [
    {
      "src": "/api/.*",
      "headers": {
        "Access-Control-Allow-Origin": "http://localhost:3000",
        "Access-Control-Allow-Methods": "GET,OPTIONS,PATCH,DELETE,POST,PUT",
        "Access-Control-Allow-Headers": "Authorization, X-Requested-With, Content-Type"
      }
    }
  ]
}
