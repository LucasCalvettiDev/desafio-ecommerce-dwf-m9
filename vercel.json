{
  "version": 2,
  "routes": [
    {
      "src": "/api/.*",
      "methods": ["OPTIONS", "GET", "POST", "PUT", "DELETE", "PATCH"],
      "headers": {
        "Access-Control-Allow-Origin": "http://localhost:3000",
        "Access-Control-Allow-Methods": "*",
        "Access-Control-Allow-Headers": "Authorization, Content-Type"
      },
      "continue": true
    }
  ]
}
